{
  "type": "object",
  "required": ["name"],
  "properties": {
    "name": { "type": "string", "example": "John Doe" },
    "age": { "type": "integer", "minimum": 0, "example": 30 },
    "address": {
      "type": "object",
      "properties": {
        "street": { "type": "string", "example": "123 Main St" },
        "city": { "type": "string", "example": "New York" }
      }
    }
  },
  "description": "A person"
}
